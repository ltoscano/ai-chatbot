# Copy this file to .env.local and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google Vertex AI Configuration
# Get these from your Google Cloud Console: https://console.cloud.google.com/
GOOGLE_VERTEX_AI_PROJECT_ID=your_project_id_here
GOOGLE_VERTEX_AI_LOCATION=us-central1
# Optional: Path to service account key file (if not using default credentials)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Other required environment variables
AUTH_SECRET=your_auth_secret_here
BLOB_READ_WRITE_TOKEN=your_blob_token_here
POSTGRES_URL=your_postgres_url_here
REDIS_URL=your_redis_url_here
